{
  "name": "the-sorter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "ci:build": "NODE_ENV=production vite build",
    "type-check": "tsc",
    "check": "bun lint && bun type-check",
    "ci:check": "bun lint --format json --output-file lint_report.json && bun type-check",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "fix": "bun lint:fix",
    "preview": "vite preview",
    "prepare": "panda codegen",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage.enabled",
    "test:preview": "vitest-preview && open https://localhost:5006",
    "test:dev": "bunx concurrently \"TEST_PREVIEW=true bun test:ui\" \"bun test:preview\"",
    "ci:test": "vitest --coverage.enabled",
    "analyze": "bunx vite-bundle-visualizer",
    "codegen:components": "bunx @park-ui/cli components add --all"
  },
  "dependencies": {
    "@ark-ui/react": "^3.8.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@pandacss/dev": "^0.45.1",
    "@park-ui/panda-preset": "^0.42.0",
    "@testing-library/user-event": "^14.5.2",
    "file-saver": "^2.0.5",
    "i18next": "^23.14.0",
    "lodash-es": "^4.17.21",
    "lz-string": "^1.5.0",
    "modern-screenshot": "^4.4.39",
    "path-browserify": "^1.0.1",
    "react": "^19.0.0-rc-a960b92c-20240819",
    "react-dom": "^19.0.0-rc-a960b92c-20240819",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.0.1",
    "react-icons": "^5.3.0",
    "vike": "^0.4.191",
    "vike-react": "^0.5.3",
    "vite-plugin-cjs-interop": "^2.1.2"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.2",
    "@eslint/compat": "^1.1.1",
    "@pandabox/prettier-plugin": "^0.1.3",
    "@pandacss/eslint-plugin": "^0.1.9",
    "@pandacss/studio": "^0.45.1",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "babel-plugin-react-compiler": "^0.0.0-experimental-7d62301-20240819",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-compiler": "^0.0.0-experimental-8e3b87c-20240819",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.10",
    "eslint-plugin-unused-imports": "^4.1.3",
    "file-type": "^19.4.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "vitest-preview": "^0.0.1",
    "webp-converter": "^2.3.3"
  }
}
