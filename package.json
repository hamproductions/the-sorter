{
  "name": "the-sorter",
  "private": true,
  "version": "1.18.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "ci:build": "NODE_ENV=production vite build",
    "type-check": "tsc",
    "check": "bun lint && bun type-check",
    "ci:check": "bun lint:ci && bun type-check",
    "format": "prettier --write ./src",
    "lint": "oxlint src && eslint src --concurrency=auto",
    "lint:ci": "oxlint src && eslint src --concurrency=auto --format json --output-file lint_report.json || (cat lint_report.json && exit 1)",
    "lint:fix": "oxlint src --fix && eslint src --fix --concurrency=auto",
    "fix": "bun lint:fix",
    "preview": "vite preview",
    "prepare": "panda codegen",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage.enabled",
    "test:preview": "NODE_ENV=test-preview vitest-preview && open https://localhost:5006",
    "test:dev": "bunx concurrently \"TEST_PREVIEW=true bun test:ui\" \"bun test:preview\"",
    "ci:test": "vitest --coverage.enabled",
    "analyze": "bunx vite-bundle-visualizer",
    "codegen:components": "bunx @park-ui/cli components add --all",
    "release": "release-it --ci",
    "release:patch": "release-it patch --ci",
    "release:minor": "release-it minor --ci",
    "release:major": "release-it major --ci",
    "update-version": "bun scripts/update-version-release-it.ts",
    "install-hooks": "chmod +x .git/hooks/commit-msg && chmod +x scripts/check-version.ts",
    "check-version": "bun scripts/check-version.ts",
    "data-viewer": "bun run tools/data-viewer/src/index.ts"
  },
  "dependencies": {
    "@ark-ui/react": "^4.9.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@pandacss/dev": "^1.8.1",
    "@park-ui/panda-preset": "^0.43.1",
    "@sentry/react": "^8.55.0",
    "@tanstack/react-virtual": "^3.13.18",
    "file-saver": "^2.0.5",
    "i18next": "^24.2.3",
    "lodash-es": "^4.17.22",
    "lz-string": "^1.5.0",
    "modern-screenshot": "^4.6.7",
    "path-browserify": "^1.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.7.4",
    "react-icons": "^5.5.0",
    "vike": "^0.4.252",
    "vike-react": "^0.5.13",
    "vite-plugin-cjs-interop": "^2.4.0",
    "wanakana": "^5.3.1"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.3",
    "@pandabox/prettier-plugin": "^0.1.3",
    "@pandacss/eslint-plugin": "^0.3.0",
    "@pandacss/studio": "^1.8.1",
    "@release-it/conventional-changelog": "^10.0.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/wanakana": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "babel-plugin-react-compiler": "^19.0.0-beta-ebf51a3-20250411",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "^1.39.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-compiler": "^19.0.0-beta-ebf51a3-20250411",
    "eslint-plugin-unused-imports": "^4.3.0",
    "file-type": "^20.5.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsdom": "^26.1.0",
    "oxlint": "^1.39.0",
    "postcss-prune-var": "^1.1.2",
    "prettier": "^3.8.0",
    "release-it": "^18.1.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.17",
    "vitest-localstorage-mock": "^0.1.2",
    "vitest-preview": "^0.0.3",
    "webp-converter": "^2.3.3"
  }
}
