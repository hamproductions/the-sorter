{
  "name": "the-sorter",
  "private": true,
  "version": "1.10.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun scripts/add-english-names.ts && tsc && vite build",
    "ci:build": "bun scripts/add-english-names.ts && NODE_ENV=production vite build",
    "type-check": "tsc",
    "check": "bun lint && bun type-check",
    "ci:check": "bun lint --format json --output-file lint_report.json && bun type-check",
    "lint": "oxlint src && eslint src",
    "lint:oxlint": "oxlint src",
    "lint:eslint": "eslint src",
    "lint:fix": "oxlint src --fix && eslint src --fix",
    "fix": "bun lint:fix",
    "preview": "vite preview",
    "prepare": "panda codegen",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage.enabled",
    "test:preview": "vitest-preview && open https://localhost:5006",
    "test:dev": "bunx concurrently \"TEST_PREVIEW=true bun test:ui\" \"bun test:preview\"",
    "ci:test": "vitest --coverage.enabled",
    "analyze": "bunx vite-bundle-visualizer",
    "codegen:components": "bunx @park-ui/cli components add --all",
    "release": "release-it --ci",
    "release:patch": "release-it patch --ci",
    "release:minor": "release-it minor --ci",
    "release:major": "release-it major --ci",
    "update-version": "bun scripts/update-version-release-it.ts",
    "install-hooks": "chmod +x .git/hooks/commit-msg && chmod +x scripts/check-version.ts",
    "check-version": "bun scripts/check-version.ts"
  },
  "dependencies": {
    "@ark-ui/react": "^4.8.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@pandacss/dev": "^0.51.1",
    "@park-ui/panda-preset": "^0.43.1",
    "@sentry/react": "^8.54.0",
    "baseline-browser-mapping": "^2.9.6",
    "caniuse-lite": "^1.0.30001760",
    "file-saver": "^2.0.5",
    "i18next": "^24.2.1",
    "lodash-es": "^4.17.21",
    "lz-string": "^1.5.0",
    "modern-screenshot": "^4.5.5",
    "path-browserify": "^1.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.4.0",
    "vike": "^0.4.218",
    "vike-react": "^0.5.11",
    "vite-plugin-cjs-interop": "^2.1.6",
    "wanakana": "^5.3.1"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.3",
    "@eslint/compat": "^1.2.5",
    "@pandabox/prettier-plugin": "^0.1.3",
    "@pandacss/eslint-plugin": "^0.2.4",
    "@pandacss/studio": "^0.51.1",
    "@release-it/conventional-changelog": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "14.5.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/wanakana": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^3.0.3",
    "@vitest/ui": "^3.0.3",
    "babel-plugin-react-compiler": "^19.0.0-beta-decd7b8-20250118",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "^19.0.0-beta-decd7b8-20250118",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-unused-imports": "^4.1.4",
    "file-type": "^20.0.0",
    "i18next-browser-languagedetector": "^8.0.2",
    "jsdom": "^26.0.0",
    "oxlint": "^0.16.6",
    "postcss-prune-var": "^1.1.2",
    "prettier": "^3.4.2",
    "release-it": "^18.1.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15",
    "vitest-localstorage-mock": "^0.1.2",
    "vitest-preview": "^0.0.1",
    "webp-converter": "^2.3.3"
  }
}
