{
  "name": "the-sorter",
  "private": true,
  "version": "1.15.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "ci:build": "NODE_ENV=production vite build",
    "type-check": "tsc",
    "check": "bun lint && bun type-check",
    "ci:check": "bun lint:ci && bun type-check",
    "format": "prettier --write ./src",
    "lint": "oxlint src && eslint src --concurrency=auto",
    "lint:ci": "oxlint src && eslint src --concurrency=auto --format json --output-file lint_report.json || (cat lint_report.json && exit 1)",
    "lint:fix": "oxlint src --fix && eslint src --fix --concurrency=auto",
    "fix": "bun lint:fix",
    "preview": "vite preview",
    "prepare": "panda codegen",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage.enabled",
    "test:preview": "NODE_ENV=test-preview vitest-preview && open https://localhost:5006",
    "test:dev": "bunx concurrently \"TEST_PREVIEW=true bun test:ui\" \"bun test:preview\"",
    "ci:test": "vitest --coverage.enabled",
    "analyze": "bunx vite-bundle-visualizer",
    "codegen:components": "bunx @park-ui/cli components add --all",
    "release": "release-it --ci",
    "release:patch": "release-it patch --ci",
    "release:minor": "release-it minor --ci",
    "release:major": "release-it major --ci",
    "update-version": "bun scripts/update-version-release-it.ts",
    "install-hooks": "chmod +x .git/hooks/commit-msg && chmod +x scripts/check-version.ts",
    "check-version": "bun scripts/check-version.ts",
    "data-viewer": "bun run tools/data-viewer/src/index.ts"
  },
  "dependencies": {
    "@ark-ui/react": "^4.8.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@pandacss/dev": "^1.7.1",
    "@park-ui/panda-preset": "^0.43.1",
    "@sentry/react": "^8.54.0",
    "@tanstack/react-virtual": "^3.13.16",
    "file-saver": "^2.0.5",
    "i18next": "^24.2.1",
    "lodash-es": "^4.17.21",
    "lz-string": "^1.5.0",
    "modern-screenshot": "^4.5.5",
    "path-browserify": "^1.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.4.0",
    "vike": "^0.4.249",
    "vike-react": "^0.5.11",
    "vite-plugin-cjs-interop": "^2.1.6",
    "wanakana": "^5.3.1"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.10.3",
    "@pandabox/prettier-plugin": "^0.1.3",
    "@pandacss/eslint-plugin": "^0.3.0",
    "@pandacss/studio": "^1.7.1",
    "@release-it/conventional-changelog": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "14.5.2",
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/wanakana": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.16",
    "babel-plugin-react-compiler": "^19.0.0-beta-decd7b8-20250118",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-oxlint": "^1.25.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-compiler": "^19.0.0-beta-decd7b8-20250118",
    "eslint-plugin-unused-imports": "^4.1.4",
    "file-type": "^20.0.0",
    "i18next-browser-languagedetector": "^8.0.2",
    "jsdom": "^26.0.0",
    "oxlint": "^1.25.0",
    "postcss-prune-var": "^1.1.2",
    "prettier": "^3.4.2",
    "release-it": "^18.1.2",
    "typescript": "^5.7.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.2",
    "vitest": "^4.0.14",
    "vitest-localstorage-mock": "^0.1.2",
    "vitest-preview": "^0.0.1",
    "webp-converter": "^2.3.3"
  }
}
